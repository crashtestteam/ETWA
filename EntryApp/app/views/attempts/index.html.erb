<h1>Listing attempts</h1>

<table>
  <thead>
    <tr>
      <th>Registration date</th>
      <th>Result</th>
      <th>Start</th>
      <th>End</th>
      <th>Timetable</th>
      <th>User</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @attempts.each do |attempt| %>
      <tr>
        <td><%= attempt.registration_date %></td>
        <td><%= attempt.result %></td>
        <td><%= attempt.start %></td>
        <td><%= attempt.end %></td>
        <td><%= attempt.timetable_id %></td>
        <td><%= attempt.user.name %></td>
        <td><%= link_to 'Show', attempt %></td>
        <td><%= link_to 'Edit', edit_attempt_path(attempt) %></td>
        <td><%= link_to 'Destroy', attempt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attempt', new_attempt_path %>
